<h1>All rooms</h1>
<h3>@Model.currCategory</h3>
<div class=" row mt-5 mb-2">
    @{
        foreach (var premises in Model.AllPremises)
        {
            <div class="col-lg-4">
                <img class="img-thumbnail" src="@premises.Img" alt="@premises.Name" />
                <h2>@premises.Name</h2>
                <p>@premises.ShortDescription</p>
                <p>Price: @premises.Price.ToString("c")</p>
                <p><a class=" btn btn-warning" href="#">Detail</a></p>
            </div>
        }
    }
</div>

@{
    // to run app https://localhost:44307/Premises/GetList
}