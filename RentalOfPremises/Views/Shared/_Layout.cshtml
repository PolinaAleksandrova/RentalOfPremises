<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/css/bootstrap.min.css" rel="stylesheet" type=" text/css" />
    <link href="~/css/Style.css" rel="stylesheet" type=" text/css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
            <div class="container-fluid">
                <a class="navbar-brand" href="#">Что-то</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Что-то">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <ul class="navbar-nav me-auto mb-2 mb-md-0">
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#">Что-то</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Что-то</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link disabled">Что-то</a>
                        </li>
                    </ul>
                    <form class="d-flex">
                        <input class="form-control me-2" type="search" placeholder="Что-то" aria-label="Что-то">
                        <button class="btn btn-outline-success" type="submit">Что-то</button>
                    </form>
                </div>
            </div>
        </nav>
    </header>
    <div class="container mt-5">
        @RenderBody()
    </div>
    <footer class="container">
        <p class="float-end"><a href="#">Что-то</a></p>
        <p>© 2017–2021 Company, Inc. · <a href="#">Что-тоة</a> · <a href="#">شروط الاستخدام</a></p>
